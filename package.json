{
  "name": "ready-for-review",
  "version": "3.4.0",
  "type": "module",
  "description": "homepage of podcast \"Ready for review\"",
  "keywords": [],
  "homepage": "https://github.com/ready-for-review/ready-for-review#readme",
  "bugs": {
    "url": "https://github.com/ready-for-review/ready-for-review/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ready-for-review/ready-for-review.git"
  },
  "license": "UNLICENSED",
  "author": "Daniel Zenzes",
  "main": "index.js",
  "scripts": {
    "prebuild": "test \"$CI\" = true && npx pnpm install -r --store=node_modules/.pnpm-store || echo skipping pnpm install",
    "build": "ELEVENTY_PRODUCTION=true eleventy",
    "start": "eleventy --serve",
    "debug": "DEBUG=Eleventy* eleventy --serve",
    "prepare": "husky",
    "format": "prettier --write ."
  },
  "browserslist": [
    "defaults",
    "maintained node versions",
    "> 5%"
  ],
  "devDependencies": {
    "@11ty/eleventy": "3.1.2",
    "@11ty/eleventy-img": "6.0.4",
    "@11ty/eleventy-navigation": "1.0.4",
    "@11ty/eleventy-plugin-rss": "2.0.4",
    "@cooklang/cooklang-ts": "1.2.7",
    "@tailwindcss/postcss": "4.1.11",
    "@tailwindcss/typography": "0.5.16",
    "autoprefixer": "10.4.21",
    "browserslist": "4.25.1",
    "cross-env": "7.0.3",
    "cssnano": "7.1.0",
    "date-fns": "4.1.0",
    "dotenv": "17.2.1",
    "esbuild": "0.25.8",
    "googleapis": "154.0.0",
    "husky": "9.1.7",
    "jest": "30.0.5",
    "lint-staged": "16.1.2",
    "pagefind": "^1.3.0",
    "plyr": "3.7.8",
    "postcss": "8.5.6",
    "prettier": "3.6.2",
    "rss-parser": "3.13.0",
    "string": "3.3.3",
    "tailwindcss": "4.1.11"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
