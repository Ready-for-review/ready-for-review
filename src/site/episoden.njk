{% extends "layout/base.njk" %}
{% block content %}
    <div class="max-w-screen-lg xl:max-w-screen-xl mx-auto">
        <h1 class="px-8 max-w-screen-lg xl:max-w-screen-xl mt-12 mb-0 mx-auto text-4xl font-semibold text-orange-500 leading-tight tracking-tight">Episoden</h1>

        {% if episodes.items.length %}

            {% for episode in episodes.items %}
                <div class="w-full p-8">
                    {%- include "episode-card.njk" -%}
                </div>
            {% endfor %}

        {% endif %}

    </div>
{% endblock %}